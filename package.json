{
    "name": "text-utils",
    "displayName": "text-utils",
    "description": "Simple Text Utils",
    "icon": "icon.png",
    "version": "0.0.6",
    "publisher": "qux-bbb",
    "engines": {
        "vscode": "^1.51.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/qux-bbb/text-utils"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "text-utils.extension.checkTaskList",
                "title": "Check TODO"
            },
            {
                "command": "text-utils.extension.reverseString",
                "title": "Reverse String"
            },
            {
                "command": "text-utils.extension.transformToLowercase",
                "title": "1. To Lowercase"
            },
            {
                "command": "text-utils.extension.transformToUppercase",
                "title": "2. To Uppercase"
            },
            {
                "command": "text-utils.extension.transformToTitleCase",
                "title": "3. To Titlecase"
            },
            {
                "command": "text-utils.extension.transformToUnorderedList",
                "title": "1. To UnorderedList"
            },
            {
                "command": "text-utils.extension.transformToOrderedList",
                "title": "2. To OrderedList"
            },
            {
                "command": "text-utils.extension.base64Encode",
                "title": "1. Base64 Encode"
            },
            {
                "command": "text-utils.extension.base64Decode",
                "title": "2. Base64 Decode"
            },
            {
                "command": "text-utils.extension.aesEncryptStr",
                "title": "1. AES Encrypt Str"
            },
            {
                "command": "text-utils.extension.aesDecryptStr",
                "title": "2. AES Decrypt Str"
            },
            {
                "command": "text-utils.extension.aesEncryptFile",
                "title": "3. AES Encrypt File"
            },
            {
                "command": "text-utils.extension.aesDecryptFile",
                "title": "4. AES Decrypt File"
            },
            {
                "command": "text-utils.extension.uriEncode",
                "title": "1. URI Encode"
            },
            {
                "command": "text-utils.extension.uriDecode",
                "title": "2. URI Decode"
            },
            {
                "command": "text-utils.extension.unicodeEncode",
                "title": "1. Unicode Encode"
            },
            {
                "command": "text-utils.extension.unicodeDecode",
                "title": "2. Unicode Decode"
            },
            {
                "command": "text-utils.extension.theDate",
                "title": "The Date"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "text-utils.extension.checkTaskList",
                    "when": "editorTextFocus",
                    "group": "text-utils@1"
                },
                {
                    "command": "text-utils.extension.reverseString",
                    "when": "editorHasSelection",
                    "group": "text-utils@2"
                },
                {
                    "submenu": "text-utils/case",
                    "group": "text-utils@3"
                },
                {
                    "submenu": "text-utils/list",
                    "group": "text-utils@4"
                },
                {
                    "submenu": "text-utils/base64",
                    "group": "text-utils@5"
                },
                {
                    "submenu": "text-utils/aes",
                    "group": "text-utils@6"
                },
                {
                    "submenu": "text-utils/uri",
                    "group": "text-utils@7"
                },
                {
                    "submenu": "text-utils/unicode",
                    "group": "text-utils@8"
                },
                {
                    "command": "text-utils.extension.theDate",
                    "group": "text-utils@9"
                }
            ],
            "text-utils/case":[
                {
                    "command": "text-utils.extension.transformToLowercase",
                    "when": "editorHasSelection",
                    "group": "text-utils@3"
                },
                {
                    "command": "text-utils.extension.transformToUppercase",
                    "when": "editorHasSelection",
                    "group": "text-utils@3"
                },
                {
                    "command": "text-utils.extension.transformToTitleCase",
                    "when": "editorHasSelection",
                    "group": "text-utils@3"
                }
            ],
            "text-utils/list":[
                {
                    "command": "text-utils.extension.transformToUnorderedList",
                    "when": "editorHasSelection",
                    "group": "text-utils@4"
                },
                {
                    "command": "text-utils.extension.transformToOrderedList",
                    "when": "editorHasSelection",
                    "group": "text-utils@4"
                }
            ],
            "text-utils/base64":[
                {
                    "command": "text-utils.extension.base64Encode",
                    "when": "editorHasSelection",
                    "group": "text-utils@5"
                },
                {
                    "command": "text-utils.extension.base64Decode",
                    "when": "editorHasSelection",
                    "group": "text-utils@5"
                }
            ],
            "text-utils/aes":[
                {
                    "command": "text-utils.extension.aesEncryptStr",
                    "when": "editorHasSelection",
                    "group": "text-utils@6"
                },
                {
                    "command": "text-utils.extension.aesDecryptStr",
                    "when": "editorHasSelection",
                    "group": "text-utils@6"
                },
                {
                    "command": "text-utils.extension.aesEncryptFile",
                    "group": "text-utils@6"
                },
                {
                    "command": "text-utils.extension.aesDecryptFile",
                    "group": "text-utils@6"
                }
            ],
            "text-utils/uri":[
                {
                    "command": "text-utils.extension.uriEncode",
                    "when": "editorHasSelection",
                    "group": "text-utils@7"
                },
                {
                    "command": "text-utils.extension.uriDecode",
                    "when": "editorHasSelection",
                    "group": "text-utils@7"
                }
            ],
            "text-utils/unicode":[
                {
                    "command": "text-utils.extension.unicodeEncode",
                    "when": "editorHasSelection",
                    "group": "text-utils@8"
                },
                {
                    "command": "text-utils.extension.unicodeDecode",
                    "when": "editorHasSelection",
                    "group": "text-utils@8"
                }
            ]
        },
        "submenus": [
            {
                "id": "text-utils/case",
                "label": "Case"
            },
            {
                "id": "text-utils/list",
                "label": "List"
            },
            {
                "id": "text-utils/base64",
                "label": "Base64"
            },
            {
                "id": "text-utils/aes",
                "label": "AES"
            },
            {
                "id": "text-utils/uri",
                "label": "URI"
            },
            {
                "id": "text-utils/unicode",
                "label": "Unicode"
            }
        ],
        "keybindings": [
            {
                "command": "text-utils.extension.checkTaskList",
                "key": "alt+x",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.51.0",
        "@typescript-eslint/eslint-plugin": "^4.1.1",
        "@typescript-eslint/parser": "^4.1.1",
        "eslint": "^7.9.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    }
}